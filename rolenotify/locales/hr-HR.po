#: rolenotify/rolenotify.py:17
#, docstring
msgid "\n"
"    Notify a user when they have a Role added or removed from them\n\n"
"    More detailed docs: <https://cogs.yamikaitou.dev/rolenotify.html>\n"
"    "
msgstr ""

#: rolenotify/rolenotify.py:117
#, docstring
msgid "Configure RoleNotify"
msgstr ""

#: rolenotify/rolenotify.py:121
#, docstring
msgid "\n"
"        Set the channel to output Role Notifications to\n\n"
"        Pass 0 to clear the channel\n"
"        Pass nothing to see the configured channel\n"
"        "
msgstr ""

#: rolenotify/rolenotify.py:131
msgid "Currently set to {channel_mention}"
msgstr ""

#: rolenotify/rolenotify.py:138 rolenotify/rolenotify.py:201
msgid "Channel has been cleared"
msgstr ""

#: rolenotify/rolenotify.py:141 rolenotify/rolenotify.py:204
msgid "Channel has been set"
msgstr ""

#: rolenotify/rolenotify.py:145
#, docstring
msgid "Configure settings for a Role"
msgstr ""

#: rolenotify/rolenotify.py:149
#, docstring
msgid "Display the configured settings for a Role"
msgstr ""

#: rolenotify/rolenotify.py:153
msgid "Settings for *{role}*\n"
"----------\n"
"**Method**: {method}\n"
"{channel}**Addition**: {add}\n"
"**Message**: {add_msg}\n"
"**Removal**: {rem}\n"
"**Message**: {rem_msg}"
msgstr ""

#: rolenotify/rolenotify.py:161
msgid "**Channel**: {channel_mention}\n"
msgstr ""

#: rolenotify/rolenotify.py:174
#, docstring
msgid "\n"
"        Set the notification method\n\n"
"        Valid options are `dm` and `channel`\n"
"        "
msgstr ""

#: rolenotify/rolenotify.py:185
msgid "Invalid option, please use either `dm` or `channel`"
msgstr ""

#: rolenotify/rolenotify.py:188
msgid "Notification method has been set"
msgstr ""

#: rolenotify/rolenotify.py:192
#, docstring
msgid "\n"
"        Set the channel to output Role Notifications to\n\n"
"        Pass 0 to clear the channel and use the server defined channel\n"
"        "
msgstr ""

#: rolenotify/rolenotify.py:208
#, docstring
msgid "\n"
"        Set the notification message\n\n"
"        <option> can be either `add` or `remove`\n\n"
"        Formatting options available for <message> are\n"
"        {role_name} = Role Name\n"
"        {role_mention} = Role Mention (will not ping)\n"
"        {user_name} = User's Display Name\n"
"        {user_mention} = User Mention\n\n"
"        "
msgstr ""

#: rolenotify/rolenotify.py:226
msgid "Invalid option, please use either `add` or `remove`"
msgstr ""

#: rolenotify/rolenotify.py:229
msgid "Notification Message has been set"
msgstr ""

#: rolenotify/rolenotify.py:233
#, docstring
msgid "\n"
"        Set if the notification should be sent on Role Add\n\n"
"        <state> should be any of these combinations, `on/off`, `yes/no`, `1/0`, `true/false`\n"
"        "
msgstr ""

#: rolenotify/rolenotify.py:242
msgid "Add Notificaiton has been set"
msgstr ""

#: rolenotify/rolenotify.py:246
#, docstring
msgid "\n"
"        Set if the notification should be sent on Role Remove\n\n"
"        <state> should be any of these combinations, `on/off`, `yes/no`, `1/0`, `true/false`\n"
"        "
msgstr ""

#: rolenotify/rolenotify.py:255
msgid "Remove Notificaiton has been set"
msgstr ""

