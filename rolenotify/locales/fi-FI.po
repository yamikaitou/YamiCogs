msgstr ""

#: rolenotify/rolenotify.py:140
#, docstring
msgid "Configure settings for a Role"
msgstr ""

#: rolenotify/rolenotify.py:144
#, docstring
msgid "Display the configured settings for a Role"
msgstr ""

#: rolenotify/rolenotify.py:147
msgid "Settings for *{role}*\n"
"----------\n"
"**Method**: {method}\n"
"{channel}**Addition**: {add}\n"
"**Message**: {add_msg}\n"
"**Removal**: {rem}\n"
"**Message**: {rem_msg}"
msgstr ""

#: rolenotify/rolenotify.py:153
msgid "**Channel**: {channel_mention}\n"
msgstr ""

#: rolenotify/rolenotify.py:163
#, docstring
msgid "\n"
"        Set the notification method\n\n"
"        Valid options are `dm` and `channel`\n"
"        "
msgstr ""

#: rolenotify/rolenotify.py:174
msgid "Invalid option, please use either `dm` or `channel`"
msgstr ""

#: rolenotify/rolenotify.py:177
msgid "Notification method has been set"
msgstr ""

#: rolenotify/rolenotify.py:181
#, docstring
msgid "\n"
"        Set the channel to output Role Notifications to\n\n"
"        Pass 0 to clear the channel and use the server defined channel\n"
"        "
msgstr ""

#: rolenotify/rolenotify.py:197
#, docstring
msgid "\n"
"        Set the notification message\n\n"
"        <option> can be either `add` or `remove`\n\n"
"        Formatting options available for <message> are\n"
"        {role_name} = Role Name\n"
"        {role_mention} = Role Mention (will not ping)\n"
"        {user_name} = User's Display Name\n"
"        {user_mention} = User Mention\n\n"
"        "
msgstr ""

#: rolenotify/rolenotify.py:215
msgid "Invalid option, please use either `add` or `remove`"
msgstr ""

#: rolenotify/rolenotify.py:218
msgid "Notification Message has been set"
msgstr ""

#: rolenotify/rolenotify.py:222
#, docstring
msgid "\n"
"        Set if the notification should be sent on Role Add\n\n"
"        <state> should be any of these combinations, `on/off`, `yes/no`, `1/0`, `true/false`\n"
"        "
msgstr ""

#: rolenotify/rolenotify.py:231
msgid "Add Notificaiton has been set"
msgstr ""

#: rolenotify/rolenotify.py:235
#, docstring
msgid "\n"
"        Set if the notification should be sent on Role Remove\n\n"
"        <state> should be any of these combinations, `on/off`, `yes/no`, `1/0`, `true/false`\n"
"        "
msgstr ""

